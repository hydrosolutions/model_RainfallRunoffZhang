<?xml version="1.0" encoding="UTF-8"?>
<ASCIIObserver xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<!-- 1st series uses header lines for meta data
	contents of this file look like:
	# Location    : aberdeen
	# Position    : (-2.045543,57.361939)
	# Source      : observed
	# Unit        : waterlevel_astro
	# Analyse time: most recent
	# Timezone    : GMT
	20080 0.8300
	20080 0.8800
	20080 0.9100
        ...
	-->
	<timeSeries status="use" standardDeviation="0.05" unit="mm" quantity="discharge"> <!-- status can be "use", "validate" or "ignore" -->
		testObservationShort.txt
	</timeSeries>

	<!-- 2nd series sets or overrules header lines
	Location    : aberdeen
	Position    : (-2.045543,57.361939)
	Source      : observed
	Unit        : waterlevel_astro
	Analyse time: most recent
	Timezone    : GMT
	-->
	 
	<timeSeries status="validate" location="Catchment2" position="(10.0,11.0)" source="observed2" quantity="discharge2" unit="mm2" timezone="GMT2" height="0.02" standardDeviation="0.052" >
		testObservationShort.txt
	</timeSeries>
	
</ASCIIObserver>
